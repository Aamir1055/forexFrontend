{
  "name": "user-management-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean:dist": "powershell -NoProfile -Command \"if (Test-Path dist) { Remove-Item -Recurse -Force dist }\"",
    "build": "tsc && vite build",
    "build:clean": "npm run clean:dist && npm run build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "jest --watch --verbose",
    "test:users": "jest --testPathPattern=users",
    "deploy": "powershell -ExecutionPolicy Bypass -File ./deploy.ps1",
    "deploy:backup": "powershell -ExecutionPolicy Bypass -File ./deploy.ps1 -Backup",
    "deploy:quick": "powershell -ExecutionPolicy Bypass -File ./deploy.ps1 -SkipBuild",
    "watch:deploy": "powershell -ExecutionPolicy Bypass -File ./watch-deploy.ps1",
    "push": "powershell -ExecutionPolicy Bypass -File ./auto-push.ps1",
    "push:schedule": "powershell -ExecutionPolicy Bypass -File ./auto-push-scheduler.ps1",
    "push:setup": "powershell -ExecutionPolicy Bypass -File ./setup-scheduled-task.ps1",
    "push:remove": "powershell -ExecutionPolicy Bypass -File ./remove-scheduled-task.ps1"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.546.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.6.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "recharts": "^3.3.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/whatwg-fetch": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/ui": "^4.0.6",
    "autoprefixer": "^10.4.16",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "happy-dom": "^20.0.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.1.0",
    "msw": "^2.11.6",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^4.0.6",
    "whatwg-fetch": "^3.6.20"
  }
}
